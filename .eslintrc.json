{
	"extends": "airbnb",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"spread": true
		}
	},
	"rules": {
		"no-console": 0,
		"react/jsx-one-expression-per-line": 0
	}
}
